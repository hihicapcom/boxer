<h1>Editing analysis</h1>

<% form_for(@analysis) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :keyword %><br />
    <%= f.text_field :keyword %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :reference %><br />
    <%= f.text_area :reference %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :create_at %><br />
    <%= f.datetime_select :create_at %>
  </p>
  <p>
    <%= f.label :update_at %><br />
    <%= f.datetime_select :update_at %>
  </p>
  <p>
    <%= f.label :demand_id %><br />
    <%= f.text_field :demand_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @analysis %> |
<%= link_to 'Back', analyses_path %>