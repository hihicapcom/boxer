<h1>Editing knowledge</h1>

<% form_for(@knowledge) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :analysis %><br />
    <%= f.text_area :analysis %>
  </p>
  <p>
    <%= f.label :plan %><br />
    <%= f.text_area :plan %>
  </p>
  <p>
    <%= f.label :knowledge %><br />
    <%= f.text_area :knowledge %>
  </p>
  <p>
    <%= f.label :reference %><br />
    <%= f.text_area :reference %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :create_at %><br />
    <%= f.datetime_select :create_at %>
  </p>
  <p>
    <%= f.label :update_at %><br />
    <%= f.datetime_select :update_at %>
  </p>
  <p>
    <%= f.label :doc_id %><br />
    <%= f.text_field :doc_id %>
  </p>
  <p>
    <%= f.label :project_id %><br />
    <%= f.text_field :project_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

