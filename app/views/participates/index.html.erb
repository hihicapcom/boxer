<h1>Listing participates</h1>

<table>
  <tr>
    <th>User</th>
    <th>Project</th>
  </tr>

<% @participates.each do |participate| %>
  <tr>
    <td><%=h participate.user_id %></td>
    <td><%=h participate.project_id %></td>
    <td><%= link_to 'Show', participate %></td>
    <td><%= link_to 'Edit', edit_participate_path(participate) %></td>
    <td><%= link_to 'Destroy', participate, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New participate', new_participate_path %>